{
  "manifest_version": 2,

  "name": "WIGAF",
  "description": "This extension will use your google calendar to auto-fill whenisgood pages.",
  "version": "0.0.1",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "settings.html"
  },
  "permissions": ["tabs", "*://*whenisgood.net/*"],
  "content_scripts": [
    {
      "matches": ["*://*whenisgood.net/*"],
      "js": ["autofill.js"]
    }
  ]
}
